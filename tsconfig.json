{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"allowImportingTsExtensions": true,
		"allowJs": true,
		"checkJs": true,
		"strict": true,
		"noEmit": true,
		"sourceMap": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true
	},
	"include": [
		"src/**/*",
		"vite.config.ts",
		"vitest.config.ts",
		"vitest.workspace.ts"
	]
}
